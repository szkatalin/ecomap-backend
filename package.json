{
   "name": "ecomap-backend",
   "version": "0.0.1",
   "description": "<p align=\"center\">   <a href=\"http://nestjs.com/\" target=\"blank\"><img src=\"https://nestjs.com/img/logo_text.svg\" width=\"320\" alt=\"Nest Logo\" /></a> </p>",
   "author": "",
   "license": "MIT",
   "scripts": {
      "start": "ts-node src/index.ts",
      "start:debug": "nodemon --config nodemon-debug.json",
      "start:watch": "nodemon",
      "foreman": "foreman start -f Procfile.dev",
      "prestart:prod": "tsc",
      "start:prod": "node dist/main.js",
      "migration:run": "ts-node ./node_modules/typeorm/cli.js migration:run",
      "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert",
      "migration:run:prod": "tsc && ts-node ./node_modules/typeorm/cli.js migration:run",
      "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js",
      "migrations:generate": "typeorm migration:generate -n migration"
   },
   "dependencies": {
      "@nestjs/common": "^6.11.8",
      "@nestjs/core": "^6.11.8",
      "@nestjs/jwt": "^6.1.2",
      "@nestjs/microservices": "^6.11.8",
      "@nestjs/passport": "^6.2.0",
      "@nestjs/platform-express": "^6.11.8",
      "@nestjs/swagger": "^4.3.1",
      "@nestjs/testing": "^6.11.8",
      "@nestjs/typeorm": "^6.3.3",
      "@nestjs/websockets": "^6.11.8",
      "@types/express": "4.16.1",
      "@types/jest": "^25.1.3",
      "@types/node": "^12.0.0",
      "class-validator": "^0.8.1",
      "cors": "^2.8.4",
      "dotenv": "^8.0.0",
      "express-jwt": "^5.3.1",
      "jsonwebtoken": "^8.5.0",
      "jwks-rsa": "^1.7.0",
      "morgan": "^1.9.0",
      "mysql": "^2.14.1",
      "passport": "^0.4.1",
      "passport-jwt": "^4.0.0",
      "pg": "^7.18.2",
      "redis": "^2.8.0",
      "reflect-metadata": "^0.1.10",
      "request": "^2.88.0",
      "rxjs": "^6.5.0",
      "swagger-ui-express": "^4.1.3",
      "ts-node": "~8.2.0",
      "typeorm": "0.2.24",
      "typescript": "~3.5.0"
   },
   "devDependencies": {
      "@types/jsonwebtoken": "^8.3.2",
      "@types/node": "^8.0.29",
      "@types/passport-jwt": "^3.0.1",
      "nodemon": "^1.19.0",
      "prettier": "^1.19.1",
      "ts-node": "3.3.0",
      "tslint": "^5.17.0",
      "tslint-config-prettier": "^1.18.0",
      "typescript": "3.3.3333"
   },
   "engines": {
      "node": "12"
   },
   "jest": {
      "moduleFileExtensions": [
         "js",
         "json",
         "ts"
      ],
      "rootDir": "src",
      "testRegex": ".spec.ts$",
      "transform": {
         "^.+\\.(t|j)s$": "ts-jest"
      },
      "coverageDirectory": "../coverage",
      "testEnvironment": "node"
   },
   "main": "index.js",
   "directories": {
      "test": "test"
   },
   "repository": {
      "type": "git",
      "url": "git+https://github.com/szkatalin/ecomap-backend.git"
   },
   "keywords": [],
   "bugs": {
      "url": "https://github.com/szkatalin/ecomap-backend/issues"
   },
   "homepage": "https://github.com/szkatalin/ecomap-backend#readme"
}
